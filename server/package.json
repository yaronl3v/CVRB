{
  "name": "cvrb-creator",
  "version": "1.0.0",
  "description": "CVRB Creator component for generating tasks using LLMs",
  "type": "module",
  "scripts": {
    "dev": "node src/server.js",
    "start": "node src/server.js",
    "create-db": "node src/tools/create-database.js",
    "build-db": "node src/db_scripts/build_db.js",
    "populate-db": "node src/db_scripts/import_db.js",
    "backup-db": "node src/db_scripts/export_db.js",
    "setup": "npm install && npm run create-db && npm run build-db && npm run populate-db"
  },
  "keywords": [
    "CVRB",
    "LLM",
    "benchmark"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "json5": "^2.2.3",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  }
}
